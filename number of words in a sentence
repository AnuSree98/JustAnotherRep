#include<stdio.h>
int main()
{
   char a;
   int i=0,c;
   scanf("%[^\n]s",a);
   if(a[0]==" ")
   {
      c=0;
   }
   else
   {
      c=1;
   }
   while(a[i]!='\0')
   {
      if(a[i]==' ' && a[i+1]!=' ' && a[i+1]!='\0')
      {
          c=c+1;
      }
      i++;
    }
    printf("The count is %d",c);
  }
   
